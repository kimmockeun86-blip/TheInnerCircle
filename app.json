{
  "expo": {
    "name": "ORBIT",
    "slug": "TheInnerCircle",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000020"
    },
    "ios": {
      "bundleIdentifier": "com.theinnercircle.app",
      "supportsTablet": false,
      "buildNumber": "52",
      "infoPlist": {
        "NSCameraUsageDescription": "리추얼 기록 사진 촬영에 사용됩니다.",
        "NSPhotoLibraryUsageDescription": "리추얼 기록에 사진을 첨부하기 위해 사용됩니다.",
        "NSLocationWhenInUseUsageDescription": "가까운 거리의 인연을 찾기 위해 위치를 사용합니다.",
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "ko"
      },
      "entitlements": {
        "aps-environment": "production"
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "locales": {
      "ko": "./locales/ko.json"
    },
    "android": {
      "package": "com.theinnercircle.app",
      "versionCode": 11,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000020"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#FF00FF",
          "sounds": []
        }
      ],
      "expo-audio"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "https://theinnercircle-9xye.onrender.com/api",
      "appEnv": "production",
      "eas": {
        "projectId": "c071b646-217f-41ad-9839-4b44dd28257a"
      }
    },
    "owner": "orbitappkorea"
  }
}